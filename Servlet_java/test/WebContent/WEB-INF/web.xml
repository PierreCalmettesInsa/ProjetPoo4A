<?xml version="1.0" encoding="UTF-8"?>
<web-app
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" 
	version="3.0">

	
	<servlet>
		<servlet-name>Subscribe</servlet-name>
		<servlet-class>com.sdzee.servlets.Subscribe</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>Subscribe</servlet-name>
		<url-pattern>/subscribe</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Publish</servlet-name>
		<servlet-class>com.sdzee.servlets.Publish</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>Publish</servlet-name>
		<url-pattern>/publish</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Notify</servlet-name>
		<servlet-class>com.sdzee.servlets.Notify</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>Notify</servlet-name>
		<url-pattern>/notify</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>OpenConnection</servlet-name>
		<servlet-class>com.sdzee.servlets.OpenConnection</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>OpenConnection</servlet-name>
		<url-pattern>/openConnection</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Message</servlet-name>
		<servlet-class>com.sdzee.servlets.Message</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>Message</servlet-name>
		<url-pattern>/message</url-pattern>
	</servlet-mapping>
	
	
	
	<servlet>
		<servlet-name>FileMessage</servlet-name>
		<servlet-class>com.sdzee.servlets.FileMessage</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>FileMessage</servlet-name>
		<url-pattern>/fileMessage</url-pattern>
	</servlet-mapping>
	

	
	

</web-app>